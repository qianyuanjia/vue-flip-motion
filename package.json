{
  "name": "vue-flip",
  "version": "0.0.0",
  "description": "A Vue component based on FLIP (First Last Invert Play) animation technique that makes your animations as smooth as silk!",
  "keywords": [
    "flip",
    "animation",
    "vue",
    "component"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/flip.cjs"
    }
  },
  "homepage": "https://github.com/qianyuanjia/vue-flip#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qianyuanjia/vue-flip.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepublishOnly": "rollup --config rollup.config.js && rollup --config src/components/Flip/vue2/rollup.config.js",
    "pub": "npm version patch && npm publish --registry=https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "author": "huanyue",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^2.6.10 || ^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-vue": "^6.0.0",
    "vite": "^7.1.6",
    "vue": "^3.5.21"
  }
}
